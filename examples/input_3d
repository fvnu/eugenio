CONFIG
FINALTOL: 1.e-13;
IMAGTHRESHOLD: 1.e-8;
MPTYPE: 2;
COEFFBOUND: 1000;
DEGREEBOUND: 5;
RANDOMSEED: 20180305;
SCREENOUT: 0;
END;
INPUT
variable_group x1, x2, x3;
function f1, f2, f3;
f1 = 8*x1*(-1 + x1^2 + x2^2 + x3^2);
f2 = -0.25 + 8*x2*(-1 + x1^2 + x2^2 + x3^2);
f3 = 3 + 8*x3*(-1 + x1^2 + x2^2 + x3^2);
END;
